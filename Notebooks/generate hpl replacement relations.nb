Cell[BoxData[{
 RowBox[{
  RowBox[{"countLyndonBasis", "[", 
   RowBox[{"n_", ",", "alphabet_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"m", "=", 
       RowBox[{"Length", "[", "alphabet", "]"}]}], ",", 
      RowBox[{"d", "=", 
       RowBox[{"Divisors", "@", "n"}]}]}], "}"}], ",", 
    RowBox[{"Plus", "@@", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"MoebiusMu", "[", 
        RowBox[{"n", "/", "d"}], "]"}], "*", 
       RowBox[{
        RowBox[{"m", "^", "d"}], "/", "n"}]}], ")"}]}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"generateLyndonBasis", "[", 
    RowBox[{
     RowBox[{"{", "n__", "}"}], ",", "alphabet_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"fullBasis", "=", 
       RowBox[{"generateLyndonBasis", "[", 
        RowBox[{
         RowBox[{"Max", "[", 
          RowBox[{"List", "[", "n", "]"}], "]"}], ",", "alphabet"}], "]"}]}], 
      "}"}], ",", 
     RowBox[{"Sort", "[", 
      RowBox[{"Join", "@@", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Select", "[", 
          RowBox[{"fullBasis", ",", 
           RowBox[{
            RowBox[{
             RowBox[{"Length", "[", "#", "]"}], "==", "l"}], "&"}]}], "]"}], 
         ",", 
         RowBox[{"{", 
          RowBox[{"l", ",", 
           RowBox[{"List", "[", "n", "]"}]}], "}"}]}], "]"}]}], "]"}]}], 
    "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"generateLyndonBasis", "[", 
     RowBox[{"n_", ",", "alphabet_"}], "]"}], ":=", " ", 
    RowBox[{
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"w", "=", 
          RowBox[{"{", 
           RowBox[{"alphabet", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "}"}]}], ",", "i", ",", "temp", 
         ",", 
         RowBox[{"list", "=", 
          RowBox[{"{", 
           RowBox[{"{", 
            RowBox[{"alphabet", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "}"}], "}"}]}], ",", "pos", ",", 
         
         RowBox[{"j", "=", "1"}], ",", 
         RowBox[{"k", "=", "0"}], ",", "max"}], "}"}], ",", "\n", "\t", 
       RowBox[{
        RowBox[{"max", "=", 
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{"countLyndonBasis", "[", 
            RowBox[{"k", ",", "alphabet"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", "\n", 
        "\t", 
        RowBox[{
         RowBox[{"Reap", "[", "\n", "\t\t", 
          RowBox[{
           RowBox[{"Sow", "[", "w", "]"}], ";", "\n", "\t\t", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"j", "=", "1"}], ",", 
             RowBox[{"j", "<", "max"}], ",", 
             RowBox[{"j", "++"}], ",", "\n", "\t\t\t", 
             RowBox[{
              RowBox[{"temp", "=", 
               RowBox[{"PadRight", "[", 
                RowBox[{
                 RowBox[{"{", "}"}], ",", "n", ",", "w"}], "]"}]}], ";", "\n",
               "\t\t\t", 
              RowBox[{"While", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"temp", "[", 
                  RowBox[{"[", 
                   RowBox[{"-", "1"}], "]"}], "]"}], "==", 
                 RowBox[{"alphabet", "[", 
                  RowBox[{"[", 
                   RowBox[{"-", "1"}], "]"}], "]"}]}], ",", 
                RowBox[{"temp", "=", 
                 RowBox[{"Drop", "[", 
                  RowBox[{"temp", ",", 
                   RowBox[{"-", "1"}]}], "]"}]}]}], "]"}], ";", "\n", 
              "\t\t\t", 
              RowBox[{"pos", "=", 
               RowBox[{
                RowBox[{"Flatten", "[", 
                 RowBox[{"Position", "[", 
                  RowBox[{"alphabet", ",", 
                   RowBox[{"temp", "[", 
                    RowBox[{"[", 
                    RowBox[{"-", "1"}], "]"}], "]"}]}], "]"}], "]"}], "[", 
                RowBox[{"[", "1", "]"}], "]"}]}], ";", "\n", "\t\t\t", 
              RowBox[{
               RowBox[{"temp", "[", 
                RowBox[{"[", 
                 RowBox[{"-", "1"}], "]"}], "]"}], "=", 
               RowBox[{"alphabet", "[", 
                RowBox[{"[", 
                 RowBox[{"pos", "+", "1"}], "]"}], "]"}]}], ";", "\n", 
              "\t\t\t", 
              RowBox[{"Sow", "[", 
               RowBox[{"w", "=", "temp"}], "]"}], ";"}]}], "]"}], ";"}], 
          "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"2", ",", "1"}], "]"}], "]"}]}]}], "]"}], "/;", 
     RowBox[{"!", 
      RowBox[{"MatchQ", "[", 
       RowBox[{"n", ",", 
        RowBox[{"List", "[", "__", "]"}]}], "]"}]}]}]}], ";"}], 
  RowBox[{"(*", 
   RowBox[{"from", " ", "Jeff"}], "*)"}]}]}], "Input",
